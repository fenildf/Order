<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style src="xxx.css" field="new2"></ui:style>
	<ui:style src="new2.css" field="new1"></ui:style>
	<g:HTMLPanel>


		<div class="{new2.mainpage}">

			<div class="{new2.header}">
				<div class="{new2.restaurantlogo}">
					<a href="/">
						<img src="/app/www/images/logo.gif" alt="logo" border="0" />
					</a>
				</div>
				<div class="{new2.progressbar}">
					<ul>
						<li>1、选择餐厅</li>
						<li class="{new2.current}">2、选菜</li>
						<li>3、确认订单</li>
					</ul>
				</div>
				<div class="{new2.restaurantline} {new2.cb}"></div>
				<!--网站当前位置 -->
				<div class="{new2.site-place} {new2.clearfix}">
					<p class="{new2.sp-link}">
						<a href="/">首页</a>
						&gt;
						<a href="#">餐厅</a>
						&gt;
						<a href="#">自助预订</a>
					</p>
					<p class="{new2.order-hotline} {new2.clearfix}">
						<span>预订电话：</span>
						<span class="{new2.phone-num}">40008888888</span>
					</p>
				</div>
			</div>

			<div class="{new2.content}">
				<div class="{new2.bookinginformation}">
					<h3>用户预订信息：</h3>
					<div class="{new1.table}">
						<g:FlowPanel ui:field="bookingPanel" />
					</div>


					<h3>选菜信息：</h3>
					<div>菜品类别：<g:InlineLabel ui:field="selectTypeTitle" styleName="{new1.selectTypefont}" /> </div>
					<dl>
						<dt style="width:100%;height:30px;text-align: left;">
							
							<g:FlowPanel ui:field="tabpage" />
						</dt>

					</dl>
					<dl>
						<dt style="width:100%;height:100%;text-align: left;">
							<div class="{new1.table}">
								<g:FlowPanel ui:field="resultPanel" />
							</div>


							<div id="rewardlist_page2" class="{new1.pager}">
								<div class="{new1.pager-left}">
									每页显示条数
									<select class="{new2.select}">
										<option>10</option>
									</select>
								</div>

								<div class="{new1.pager-right}">
									<g:FlowPanel ui:field="resultpage" />
								</div>
							</div>
						</dt>

					</dl>

					<dl>
						<g:FlowPanel ui:field="detailPanel" />
					</dl>
					<dl>
						<dt></dt>
						<dd>
							<input type="hidden" name="s" value="biz" />
							<input type="hidden" name="a" value="submitbizorder" />
							<input type="hidden" name="bid" value="87520" />
							<input type="hidden" name="cid" value="" />
							<input type="hidden" name="f" value="" />

							<g:Button ui:field="returnBtn" text="返回修改预定信息" />
							<g:Button ui:field="addBtn" text="立刻预定" />
						</dd>
					</dl>

				</div>
			</div>

		</div>
	</g:HTMLPanel>
</ui:UiBinder> 